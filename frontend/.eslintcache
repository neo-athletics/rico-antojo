iv>\n                <Row className={\"itemContainer align-content-center\"}>\n                  <Col lg={6}>\n                    <span>{item.description}</span>\n                  </Col>\n                  <Col>\n                    <span>\n                      ${parseFloat(item.price * item.quantity).toFixed(2)}\n                    </span>\n                  </Col>\n                  <Col>\n                    <select\n                      name={\"quantity\"}\n                      ref={register}\n                      value={item.quantity}\n                      onChange={(e) => handleChange(e, item)}\n                    >\n                      {[...Array(10)].map((opt, i) => (\n                        <option value={i + 1}>{i + 1}</option>\n                      ))}\n                    </select>\n                    <button\n                      className={\"removeItem\"}\n                      onClick={() => dispatch(removeItem(item))}\n                    ></button>\n                  </Col>\n                </Row>\n              </>\n            ))}\n          </Col>\n          <Col className={\"d-flex justify-content-center\"}>\n            <span>\n              Total ${parseFloat(cart.reduce(reducer, 0.0)).toFixed(2)}\n            </span>\n          </Col>\n        </Row>\n      </Container>\n    </>\n  );\n};\n\nexport default Cart;\n","/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/Home.js",[],"/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/Menu.js",[],"/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/NavBar.js",["70","71"],"import React from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { Navbar, Nav, Container, NavDropdown } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\nimport SideCart from \"./SideCart\";\nimport DropdownToggle from \"react-bootstrap/esm/DropdownToggle\";\n\nconst NavBar = () => {\n  const cartLength = useSelector((state) => state.cart.length);\n\n  return (\n    <Navbar bg=\"primary\" variant=\"dark\" className=\"mb-5\">\n      <Navbar.Brand href=\"#home\">Rico Antojo</Navbar.Brand>\n      <Container className=\"justify-content-end\">\n        <Nav>\n          <Nav.Link as={Link} to=\"/\">\n            Home\n          </Nav.Link>\n          <Nav.Link as={Link} to=\"/menu\">\n            Menu\n          </Nav.Link>\n          <Nav.Link as={Link} to=\"/about\">\n            About\n          </Nav.Link>\n          <div className=\"dropdown nav-item\">\n            <Nav.Link as={Link} to=\"/cart\" className={\"cart\"}>\n              Cart\n              <span className={\"itemCount\"}>\n                <span>{cartLength}</span>\n              </span>\n            </Nav.Link>\n            <SideCart />\n          </div>\n        </Nav>\n      </Container>\n    </Navbar>\n  );\n};\n\nexport default NavBar;\n","/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/About.js",[],"/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/ItemForm.js",[],"/Volumes/OrionTech/code-neo/Projects/rico-antojo/client/src/components/ToastNotification.js",["72"],"import React, { useEffect } from \"react\";\nimport Toast from \"react-bootstrap/Toast\";\nimport { useSelector } from \"react-redux\";\nconst ToastNotification = ({ setShow, show }) => {\n  const cart = useSelector((state) => state.cart);\n  console.log(cart[cart.length - 1]);\n  const item = cart[cart.length - 1] ?? {};\n\n  return (\n    <Toast\n      style={{\n        position: \"absolute\",\n        top: \"10px\",\n        right: \"10px\",\n      }}\n      onClose={() => setShow(false)}\n      show={show}\n      delay={2000}\n      autohide\n    >\n      <Toast.Header>{item.description}</Toast.Header>\n      <Toast.Body>\n        qty: {item.quantity} ea.${parseFloat(item.price).toFixed(2)}\n      </Toast.Body>\n    </Toast>\n  );\n};\n\nexport default ToastNotification;\n","/Volumes/OrionTech/code-neo/P