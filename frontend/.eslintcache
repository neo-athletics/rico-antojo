     history.push(\"/\");\n    };\n\n    return (\n        <Navbar bg=\"primary\" variant=\"dark\" className=\"mb-5\">\n            <Navbar.Brand href=\"#home\">Rico Antojo</Navbar.Brand>\n            <Container className=\"justify-content-end\">\n                <Nav>\n                    <Nav.Link as={Link} to=\"/\">\n                        Home\n                    </Nav.Link>\n                    <Nav.Link as={Link} to=\"/menu\">\n                        Menu\n                    </Nav.Link>\n                    <Nav.Link as={Link} to=\"/about\">\n                        About\n                    </Nav.Link>\n                    <div className=\"dropdown nav-item\">\n                        <Nav.Link as={Link} to=\"/cart\" className={\"cart\"}>\n                            Cart\n                            <span className={\"itemCount\"}>\n                                <span>{cartLength}</span>\n                            </span>\n                        </Nav.Link>\n                        <SideCart />\n                    </div>\n                    {!userStatus.status && (\n                        <>\n                            <Nav.Link as={Link} to=\"/signup\">\n                                Sign Up\n                            </Nav.Link>\n                            <Nav.Link as={Link} to=\"/login\">\n                                Log In\n                            </Nav.Link>\n                        </>\n                    )}\n                    {userStatus.status === \"success\" && (\n                        <Nav.Link onClick={handleLogOut}>Log Out</Nav.Link>\n                    )}\n                </Nav>\n            </Container>\n        </Navbar>\n    );\n};\n\nexport default NavBar;\n","/Volumes/ORION/Projects/rico-antojo/frontend/src/components/About.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/Menu.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/Home.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/Cart.js",["104","105"],"import React from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { useForm } from \"react-hook-form\";\nimport { updateItemQty, removeItem } from \"../actions/actions\";\nimport { Button, Container, Row, Col } from \"react-bootstrap\";\n// import SideCart from \"./SideCart\";\n\nconst Cart = () => {\n  const cart = useSelector((state) => state.cart);\n\n  const { register, handleSubmit, watch } = useForm();\n\n  const quantity = watch(\"quantity\");\n  const dispatch = useDispatch();\n\n  const handleChange = (e, item) => {\n    console.log(e.target.value);\n    dispatch(updateItemQty(item, e.target.value));\n  };\n  const reducer = (accu, curr) => {\n    return accu + curr.price * curr.quantity;\n  };\n  console.log(cart, quantity);\n  return (\n    <>\n      <Container>\n        {/* <SideCart /> */}\n        <Row>\n          <h1>Ready to check out</h1>\n        </Row>\n        <Row>\n          <Col style={{ paddingLeft: 0 }} lg={9}>\n            {cart.map((item) => (\n              <>\n                <div className={\"itemLine\"}></div>\n                <Row className={\"itemContainer align-content-center\"}>\n                  <Col lg={6}>\n                    <span>{item.description}</span>\n                  </Col>\n                  <Col>\n                    <span>\n                      ${parseFloat(item.price * item.quantity).toFixed(2)}\n                    </span>\n                  </Col>\n                  <Col>\n                    <select\n                      // name={\"quantity\"}\n                      // ref={register}\n                      {...register(\"quantity\")}\n                      value={item.quantity}\n                      onChange={(e) => handleChange(e, item)}\n                    >\n                      {[...Array(10)].map((opt, i) => (\n                        <option value={i + 1}>{i + 1}</option>\n                      ))}\n                    </select>\n                    <button\n                      className={\"removeItem\"}\n                      onClick={() => dispatch(removeItem(item))}\n                    ></button>\n                  </Col>\n                </Row>\n              </>\n            ))}\n          </Col>\n          <Col className={\"d-flex justify-content-center\"}>\n            <span>\n              Total ${parseFloat(cart.reduce(reducer, 0.0)).toFixed(2)}\n            </span>\n          </Col>\n        </Row>\n      </Container>\n    </>\n  );\n};\n\nexport default Cart;\n","/Volumes/ORION/Projects/rico-antojo/frontend/src/components/SignUp.js",["106","107"],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/LogIn.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/menuItems.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/SideCart.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/ItemForm.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/components/ToastNotification.js",["108"],"import React, { useEffect } from \"react\";\nimport Toast from \"react-bootstrap/Toast\";\nimport { useSelector } from \"react-redux\";\nconst ToastNotification = ({ setShow, show }) => {\n  const cart = useSelector((state) => state.cart);\n  console.log(cart[cart.length - 1]);\n  const item = cart[cart.length - 1] ?? {};\n\n  return (\n    <Toast\n      style={{\n        position: \"absolute\",\n        top: \"10px\",\n        right: \"10px\",\n      }}\n      onClose={() => setShow(false)}\n      show={show}\n      delay={2000}\n      autohide\n    >\n      <Toast.Header>{item.description}</Toast.Header>\n      <Toast.Body>\n        qty: {item.quantity} ea.${parseFloat(item.price).toFixed(2)}\n      </Toast.Body>\n    </Toast>\n  );\n};\n\nexport default ToastNotification;\n","/Volumes/ORION/Projects/rico-antojo/frontend/src/components/Item.js",[],"/Volumes/ORION/Projects/rico-antojo/frontend/src/actions/actions.js",[],"/Volumes/ORION/Projects