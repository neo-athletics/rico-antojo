t);\n\n  const { register, handleSubmit, watch } = useForm();\n\n  const quantity = watch(\"quantity\");\n  const dispatch = useDispatch();\n\n  const handleChange = (e, item) => {\n    console.log(e.target.value);\n    dispatch(updateItemQty(item, e.target.value));\n  };\n  const reducer = (accu, curr) => {\n    return accu + curr.price * curr.quantity;\n  };\n  console.log(cart, quantity);\n  return (\n    <>\n      <Container>\n        {/* <SideCart /> */}\n        <Row>\n          <h1>Ready to check out</h1>\n        </Row>\n        <Row>\n          <Col style={{ paddingLeft: 0 }} lg={9}>\n            {cart.map((item) => (\n              <>\n                <div className={\"itemLine\"}></div>\n                <Row className={\"itemContainer align-content-center\"}>\n                  <Col lg={6}>\n                    <span>{item.description}</span>\n                  </Col>\n                  <Col>\n                    <span>\n                      ${parseFloat(item.price * item.quantity).toFixed(2)}\n                    </span>\n                  </Col>\n                  <Col>\n                    <select\n                      // name={\"quantity\"}\n                      // ref={register}\n                      {...register(\"quantity\")}\n                      value={item.quantity}\n                      onChange={(e) => handleChange(e, item)}\n                    >\n                      {[...Array(10)].map((opt, i) => (\n                        <option value={i + 1}>{i + 1}</option>\n                      ))}\n                    </select>\n                    <button\n                      className={\"removeItem\"}\n                      onClick={() => dispatch(removeItem(item))}\n                    ></button>\n                  </Col>\n                </Row>\n              </>\n            ))}\n          </Col>\n          <Col className={\"d-flex justify-content-center\"}>\n            <span>\n              Total ${parseFloat(cart.reduce(reducer, 0.0)).toFixed(2)}\n            </span>\n          </Col>\n        </Row>\n      </Container>\n    </>\n  );\n};\n\nexport default Cart;\n","/Volumes/ORION/Projects/rico-antojo/frontend/src/components/SignUp.js",["89"],"import React, { useEffect } from \"react\";\nimport axios from \"axios\";\nimport { useForm } from \"react-hook-form\";\n\nconst SignUp = () => {\n  const { register, handleSubmit } = useForm();\n\n  const getErrors = async () => {\n    const results = await axios.post({ method: \"post\", url: \"/signup\" });\n    return results;\n  };\n  useEffect(() => {\n    try {\n      const errors = getErrors();\n      console.log(errors);\n    } catch (err) {\n      console.log(err);\n    }\n  }, []);\n\n  const onSubmit = (data) => {\n    console.log(data);\n  };\n  return (\n    <div>\n      <h1>Sign Up</h1>\n      <form onSubmit={handleSubmit()} action=\"/signup\" method=\"POST\">\n        <label htmlFor=\"username\">username</label>\n        <input\n          type=\"text\"\n          id=\"username\"\n          name=\"username\"\n          minLength=\"8\"\n          ref={register(\"username\", { required: true })}\n        />\n        <label htmlFor=\"email\">Email</label>\n        <input\n          type=\"email\"\n          id=\"email\"\n          name=\"email\"\n          ref={register(\"email\", { required: true })}\n        />\n        <label htmlFor=\"password\">password</label>\n        <input\n          type=\"password\"\n          id=\"password\"\n          name=\"password\"\n          minLength=\"8\"\n          ref={register(\"password\", { required: true })}\n        />\n        <label htmlFor=\"password2\">re-enter password</label>\n        <input\n          type=\"password\"\n          id=\"password2\"\n          name=\"password2\"\n          minLength=\"8\"\n          ref={register(\"password2\", { required: true })}\n        />\n        <input type=\"submit\" value=\"sign up\" />\n      </form>\n    </div>\n  );\n};\n\nexport default SignUp;\n","/