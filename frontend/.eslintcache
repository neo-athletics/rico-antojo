                <Elements stripe={stripePromise} options={options}>\n                            <CardSection />\n                        </Elements>\n                    )}\n                </Route>\n                <Route path={\"/successful_payment\"}>\n                    {options.clientSecret && (\n                        <Elements stripe={stripePromise} options={options}>\n                            <SuccessfulPayment />\n                        </Elements>\n                    )}\n                </Route>\n                <Route path=\"/signup\">\n                    <SignUp setShow={setShow} />\n                </Route>\n                <Route path=\"/login\">\n                    <LogIn setShow={setShow} />\n                </Route>\n            </Switch>\n        </div>\n    );\n}\n\nexport default App;\n","/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/localStorage.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/reducers/reducers.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/LogInToast.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/Home.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/NavBar.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/ProtectedRoute.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/LogIn.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/Modal.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/Menu.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/About.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/Cart.js",["136","137","138"],"import React from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { useForm } from \"react-hook-form\";\nimport { updateItemQty, removeItem } from \"../actions/actions\";\nimport { Button, Container, Row, Col, Nav } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\nimport { useHistory } from \"react-router\";\n\nconst Cart = () => {\n    const { cart, userStatus } = useSelector((state) => state);\n    const history = useHistory();\n\n    const { register, handleSubmit, watch } = useForm();\n\n    const quantity = watch(\"quantity\");\n    const dispatch = useDispatch();\n\n    const handleChange = (e, item) => {\n        console.log(e.target.value);\n        dispatch(updateItemQty(item, e.target.value));\n    };\n    const reducer = (accu, curr) => {\n        return accu + curr.price * curr.quantity;\n    };\n    console.log(cart, quantity);\n\n    const handleClick = () => {\n        history.push(\"/cart\");\n    };\n    const discount = parseFloat(\n        cart.reduce(reducer, 0.0) - cart.reduce(reducer, 0.0) * 0.1\n    ).toFixed(2);\n\n    return (\n        <>\n            <Container className=\"checkout-container\">\n                <Row>\n                    <h1>Ready to check out</h1>\n                </Row>\n                <Row>\n                    <Col style={{ paddingLeft: 0 }} md={12} lg={12} xl={12}>\n                        {cart.length === 0 && (\n                            <p>There are currently no items in your cart</p>\n                        )}\n                        {cart.map((item) => (\n                            <>\n                                <div className={\"itemLine\"}></div>\n                                <Row\n                                    className={\n                                        \"itemContainer align-content-center\"\n                                    }\n                                >\n                                    <Col lg={6}>\n                                        <span>{item.description}</span>\n                                    </Col>\n                                    <Col>\n                                        <span>\n                                            $\n                                            {parseFloat(\n                                                item.price * item.quantity\n                                            ).toFixed(2)}\n                                        </span>\n                                    </Col>\n                                    <Col sm={3}>\n                                        <select\n                                            {...register(\"quantity\")}\n                                            value={item.quantity}\n                                            onChange={(e) =>\n                                                handleChange(e, item)\n                                            }\n                                        >\n                                            {[...Array(10)].map((opt, i) => (\n                                                <option value={i + 1}>\n                                                    {i + 1}\n                                                </option>\n                                            ))}\n                                        </select>\n                                        <button\n                                            className={\"removeItem\"}\n                                            onClick={() =>\n                                                dispatch(removeItem(item))\n                                            }\n                                        ></button>\n                                    </Col>\n                                </Row>\n                            </>\n                        ))}\n                    </Col>\n                </Row>\n                {!userStatus.status && (\n                    <Row className=\"promo-div\">\n                        <p>\n                            <Link to={\"/login\"}>Log In</Link> and get 10% off!\n                            or\n                            <Link to={\"/signup\"}> Sign Up</Link> it's free!\n                        </p>\n                    </Row>\n                )}\n                <Row md={2}>\n                    <Col style={{ paddingLeft: \"0\" }}>\n                        <Link to=\"/checkout\">\n                            <Button\n                                disabled={cart.length > 0 ? false : true}\n                                variant=\"primary\"\n                                type=\"submit\"\n                            >\n                                Check Out\n                            </Button>\n                        </Link>\n                    </Col>\n\n                    <Col\n                        style={{\n                            justifyContent: \"end\",\n                            display: \"grid\",\n                            paddingLeft: \"0\",\n                        }}\n                    >\n                        <span>\n                            Total $\n                            {userStatus.status === \"success\"\n                                ? discount\n                                : parseFloat(cart.reduce(reducer, 0.0)).toFixed(\n                                      2\n                                  )}\n                        </span>\n                    </Col>\n                </Row>\n            </Container>\n        </>\n    );\n};\n\nexport default Cart;\n","/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/SignUp.js",["139"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useForm } from \"react-hook-form\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { Redirect } from \"react-router\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { logIn } from \"../actions/logInAction\";\nimport { motion } from \"framer-motion\";\nimport { variants, item } from \"./LogIn\";\n\nconst SignUp = ({ setShow }) => {\n    const { register, handleSubmit } = useForm();\n    const userState = useSelector((state) => state.userStatus);\n    const [errors, setErrors] = useState([]);\n    const dispatch = useDispatch();\n    const history = useHistory();\n\n    if (userState.status === \"success\") {\n        console.log(\"did work\");\n        return <Redirect to={history.goBack() || \"/\"} />;\n    }\n\n    const userSignUp = async (data) => {\n        try {\n            console.log(\"sending request\");\n            const res = await axios.post(\"http://localhost:8080/signup\", data);\n            console.log(\"request send\");\n            const { message } = await res.data;\n            console.log(res.data, \"error has occurred\");\n            setErrors([]);\n            //dispatch login if there aren't any errors sent from backend\n            dispatch(\n                logIn(\n                    { username: data.username, password: data.password },\n                    setShow\n                )\n            );\n            console.log(message);\n        } catch (err) {\n            console.log(err.response, \"sign up error\");\n            const error = err?.response?.data?.errors;\n            setErrors(error);\n        }\n    };\n\n    const onSubmitForm = (data) => {\n        userSignUp(data);\n    };\n\n    return (\n        <motion.div\n            initial=\"hidden\"\n            animate=\"visible\"\n            variants={variants}\n            className=\"signup-container\"\n        >\n            <motion.h1 variants={item}>Sign Up</motion.h1>\n            {errors?.length > 0 &&\n                errors?.map((error) => (\n                    <motion.p variants={item} className=\"danger\">\n                        ! {error.msg}\n                    </motion.p>\n                ))}\n            <form\n                onSubmit={handleSubmit(onSubmitForm)}\n                action=\"/signup\"\n                method=\"post\"\n            >\n                <motion.label variants={item} htmlFor=\"username\">\n                    username\n                </motion.label>\n                <motion.input\n                    variants={item}\n                    type=\"text\"\n                    id=\"username\"\n                    name=\"username\"\n                    {...register(\"username\", { required: true })}\n                />\n                <motion.label variants={item} htmlFor=\"email\">\n                    Email\n                </motion.label>\n                <motion.input\n                    variants={item}\n                    type=\"email\"\n                    id=\"email\"\n                    name=\"email\"\n                    {...register(\"email\", { required: true })}\n                />\n                <motion.label variants={item} htmlFor=\"password\">\n                    password\n                </motion.label>\n                <motion.input\n                    variants={item}\n                    type=\"password\"\n                    id=\"password\"\n                    name=\"password\"\n                    minLength=\"8\"\n                    {...register(\"password\", { required: true })}\n                />\n                <motion.label variants={item} htmlFor=\"password2\">\n                    confirm password\n                </motion.label>\n                <motion.input\n                    variants={item}\n                    type=\"password\"\n                    id=\"password2\"\n                    name=\"password2\"\n                    minLength=\"8\"\n                    {...register(\"password2\", { required: true })}\n                />\n                <motion.input\n                    variants={item}\n                    className=\"signup-btn\"\n                    type=\"submit\"\n                    value=\"sign up\"\n                />\n            </form>\n            <motion.p\n                variants={item}\n                style={{ marginTop: \"15px\", marginBottom: \"0\" }}\n            >\n                Have an account? <Link to=\"/login\">Log In</Link>\n            </motion.p>\n        </motion.div>\n    );\n};\n\nexport default SignUp;\n","/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/CardSection.js",["140","141"],"import React, { useEffect, useState } from \"react\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport {\n    useElements,\n    useStripe,\n    PaymentElement,\n} from \"@stripe/react-stripe-js\";\nimport { Row, Col, Container } from \"react-bootstrap\";\nimport { Link } from \"react-router-dom\";\n\nconst CardSection = () => {\n    const { cart, userStatus } = useSelector((state) => state);\n    const stripe = useStripe();\n    const elements = useElements();\n    const dispatch = useDispatch();\n\n    const [message, setMessage] = useState(null);\n\n    const [isLoading, setIsLoading] = useState(false);\n\n    const reducer = (accu, curr) => {\n        return accu + curr.price * curr.quantity;\n    };\n\n    const handleSubmit = async (event) => {\n        event.preventDefault();\n\n        if (!stripe || !elements) {\n            return;\n        }\n\n        const { error } = await stripe.confirmPayment({\n            elements,\n            confirmParams: {\n                return_url: \"http://localhost:3000/successful_payment\",\n            },\n        });\n\n        if (error.type === \"card_error\" || error.type === \"validation_error\") {\n            setMessage(error.message);\n        } else {\n            setMessage(\"An unexpected error happened\");\n        }\n\n        setIsLoading(false);\n    };\n\n    const discount = parseFloat(\n        cart.reduce(reducer, 0.0) - cart.reduce(reducer, 0.0) * 0.1\n    ).toFixed(2);\n\n    return (\n        <Container>\n            <Link to=\"/cart\">Back to Cart</Link>\n            <h3 className=\"items-title\">Items</h3>\n            <Col className=\"checkout-cart\">\n                {cart.map((item) => {\n                    return (\n                        <>\n    