  },\n        });\n\n        if (payload.error) {\n            console.log(payload.error);\n        } else {\n            console.log(payload);\n        }\n    };\n    return (\n        <Container>\n            <Link to=\"/cart\">Back to Cart</Link>\n            {cart.map((item) => {\n                return (\n                    <>\n                        <p>\n                            {item.description} x {item.quantity} $\n                            {parseFloat(item.price * item.quantity).toFixed(2)}\n                        </p>\n                    </>\n                );\n            })}\n            <span>\n                Total ${parseFloat(cart.reduce(reducer, 0.0)).toFixed(2)}\n            </span>\n            <Col lg={9}>\n                <form onSubmit={handleSubmit}>\n                    <CardElement />\n                    <button type=\"submit\" disabled={!stripe}>\n                        Pay\n                    </button>\n                </form>\n            </Col>\n        </Container>\n    );\n};\nexport default CardSection;\n","/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/reducers/userStatusReducer.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/menuItems.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/SideCart.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/ItemForm.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/Item.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/components/ToastNotification.js",["113"],"import React, { useEffect } from \"react\";\nimport Toast from \"react-bootstrap/Toast\";\nimport { useSelector } from \"react-redux\";\nconst ToastNotification = ({ setShow, show }) => {\n  const cart = useSelector((state) => state.cart);\n  console.log(cart[cart.length - 1]);\n  const item = cart[cart.length - 1] ?? {};\n\n  return (\n    <Toast\n      style={{\n        position: \"absolute\",\n        top: \"10px\",\n        right: \"10px\",\n      }}\n      onClose={() => setShow(false)}\n      show={show}\n      delay={2000}\n      autohide\n    >\n      <Toast.Header>{item.description}</Toast.Header>\n      <Toast.Body>\n        qty: {item.quantity} ea.${parseFloat(item.price).toFixed(2)}\n      </Toast.Body>\n    </Toast>\n  );\n};\n\nexport default ToastNotification;\n","/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/actions/actions.js",[],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/actions/logOutAction.js",["114"],"/Users/oasistech/Documents/Projects/rico-antojo/frontend/src/actions/logInAction.js",[],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":3,"column":34,"nodeType":"121","messageId":"122","endLine":3,"endColumn":45},{"ruleId":"119","severity":1,"message":"123","line":5,"column":10,"nodeType":"121","messageId":"122","endLine":5,"endColumn":18},{"ruleId":"119","severity":1,"message":"124","line":7,"column":8,"nodeType":"121","messageId":"122","endLine":7,"endColumn":22},{"ruleId":"119","severity":1,"message":"125","line":11,"column":23,"nodeType":"121","messageId":"122","endLine":11,"endColumn":35},{"ruleId":"119","severity":1,"message":"126","line":1,"column":17,"nodeType":"121","messageId":"122","endLine":1,"endColumn":26},{"ruleId":"119","severity":1,"message":"127","line":4,"column":10,"nodeType":"121","messageId":"122","endLine":4,"endColumn":13},{"ruleId":"128","severity":1,"message":"129","line":26,"column":8,"nodeType":"130","endLine":26,"endColumn":10,"suggestions":"131"},{"ruleId":"119","severity":1,"message":"126","line":1,"column":17,"nodeType":"121","messageId":"122","endLine":1,"endColumn":26},{"ruleId":"119","severity":1,"message":"132","line":5,"column":15,"nodeType":"121","messageId":"122","endLine":5,"endColumn":21},"no-native-reassign",["133"],"no-negated-in-lhs",["134"],"no-unused-vars","'NavDropdown' is def